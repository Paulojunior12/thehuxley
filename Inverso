#include <stdio.h>

int main(){
    int i,n,N;
    scanf("%d",&N);
       int vet[N];
    for(i=0;i<N;i++){
    scanf("%d",&vet[i]);
        
    }

    for(i=0;i<N;i++){
        if(i<N-1)
            printf("%d ",vet[N-(i+1)]);
        else
            printf("%d\n",vet[N-(i+1)]);
    }

    return 0;    
}
