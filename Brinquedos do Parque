#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    int f, i, cont  = 0;
    scanf("%d %d", &f, &i);
    
    if(f >= 150 && i >= 12)
    ++cont;
    
    if(f >= 140 && i >= 14)
    ++cont;
    
    if(f >= 170 || i >= 16)
    ++cont;
    
    printf("%d\n", cont);
    
	return 0;
	
	
	
}
