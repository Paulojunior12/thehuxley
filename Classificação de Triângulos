#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    float a, b, c;
    scanf("%f %f %f",&a, &b, &c);
    
     
     if (a==b && b==c)
    printf("equilatero\n");
    
      else if(a==b || a==c || b==c)
    printf("isosceles\n");
	
     
    else
    printf("escaleno\n");
    
	return 0;
	
	
	
}
