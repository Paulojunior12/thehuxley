#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    long int n;
    scanf("%ld", &n);
    
    if( n == 0)
    printf("NULO\n");
    
    else if( n%2 == 0 && n > 0)
    printf("POSITIVO PAR\n");
    
    else if( n%2 != 0 && n > 0)
    printf("POSITIVO IMPAR  \n");
    
  
    else if( n%2 == 0 && n < 0)
    printf("NEGATIVO PAR\n");
    
    
    else if( n%2 != 0 && n < 0)
    printf("NEGATIVO IMPAR\n");
    
    return 0;
}
