#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    int interface, intelig, encap, indent, struc, tf = 1;
    scanf("%d %d %d %d %d", &interface, &intelig, &encap, &indent, &struc);
    
    if(!(interface || intelig))
    tf = 0;
    
    if(!(encap && indent))
    tf = 0;
    
    if(!struc)
    tf = 0;
    
    if(tf)
    printf("AVALIADO\n");
    
    else
    printf("%d\n", tf);
    

    
	return 0;
}
