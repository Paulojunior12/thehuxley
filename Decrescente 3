#include <stdio.h>
 int main()

{
    int x, y, z, temp;

    
    scanf("%d",&x);
    scanf("%d",&y);
    scanf("%d",&z);
  
  if(y > x){
   temp = x;
   x = y;
   y = temp;
  }
   if(z > x){
   temp = x;
   x = z;
   z = temp;
   }
   if(z > y){
   temp = y;
   y = z;
   z = temp;
   }
   printf("%d\n%d\n%d\n", x, y, z);

  
    
return 0;

}
